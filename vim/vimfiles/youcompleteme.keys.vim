"YouCompleteMe Mappings
map <C-c>lc :lclose<CR>
map <C-c>lo :lopen<CR>
map <C-c>yr :silent YcmRestartServer<CR>
map <C-c>yd :silent! call CppSyntaxCheck()<CR>
map <C-c>fix :YcmCompleter FixIt<CR>
map <C-c>nh :silent nohl<CR>
map <C-g>def :silent YcmCompleter GoToDefinition<CR>
map <C-g>dec :silent YcmCompleter GoToDeclaration<CR>
map <C-g>doc :silent Ycmcompleter GetDoc<CR>
