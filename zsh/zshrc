>>>>>>> 206c995 (Manually adding support autocd and compinit.)

# Load the oh-my-zsh's library.
zplug 'zplug/zplug', hook-build:'zplug --self-manage'

zplug "plugins/git", from:oh-my-zsh
zplug "plugins/pip", from:oh-my-zsh

zplug "plugins/command-not-found", from:oh-my-zsh
zplug 'plugins/vi-mode', from:oh-my-zsh

## Syntax highlighting bundle.
zplug "zsh-users/zsh-syntax-highlighting", defer:2
zplug "zsh-users/zsh-autosuggestions"
zplug "mafredri/zsh-async"

# Load the theme.
zplug "jackharrisonsherlock/common", as:theme

if ! zplug check --verbose; then
    printf "Install? [y/N]: "
    if read -q; then
        echo; zplug install
    fi
fi

zplug load #--verbose
#  Zplug configuration is complete. User settings start below.

alias wtf='wtf -o'
eval "$(thefuck --alias)"
alias howmanyfucks="history | grep fuck | wc -l "

#oh-my-zsh seems to enable this by default. No me gusta en macOS.
DISABLE_AUTO_TITLE="true"

if [ -d ~/.zshrc.d ]; then
	for script in `ls ~/.zshrc.d/*.zsh`; do
		source ${script}
	done
fi

# Lines configured by zsh-newuser-install
HISTFILE=~/.histfile
HISTSIZE=1000
SAVEHIST=1000
# End of lines configured by zsh-newuser-install
# The following lines were added by compinstall
zstyle :compinstall filename '/home/sdavid/.zshrc'

autoload -Uz compinit
compinit
# End of lines added by compinstall

test -e /home/sdavid/.iterm2_shell_integration.zsh && source /home/sdavid/.iterm2_shell_integration.zsh || true
